q = input()
for i in range(1,len(q)):
    if q[i-1] == 'ж' or q[i-1] == 'ш':
        if q[i] == 'ы':
            print('Ошибка в буквосочетании жи-, ши-')
        elif q[i] == 'и':
            print ('Ошибки нет')
    elif q[i-1] == 'ч' or q[i-1] == 'щ':
        if q[i] == 'я':
            print('Ошибка в буквосочетании ча-, ща-')
        elif q[i] == 'а':
            print('Ошибки нет')
            
